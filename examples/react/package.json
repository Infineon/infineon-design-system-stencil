{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@infineon/infineon-design-system-stencil": "^0.0.63",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.59.3",
    "web-vitals": "^2.1.4",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean:start": "run-s unlink start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:stencil": "npm i && cd ../ && npm run dev:stencil",
    "link": "cd ../ && npm link && cd ../examples/react && npm link @infineon/infineon-design-system-stencil",
    "unlink": "npm unlink @infineon/infineon-design-system-stencil && npm i @infineon/infineon-design-system-stencil",
    "preview:link": "run-s link start",
    "test:local": "run-p build:stencil preview:link"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}