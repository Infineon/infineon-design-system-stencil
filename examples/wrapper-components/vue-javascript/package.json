{
  "name": "vue-wrapper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:wrapper": "cd ../../../packages/components && npm i && npm run build:stencil && cd ../components-vue && npm run build",
    "link-package": "cd ../../../packages/components-vue && npm link",
    "link-example": "npm link @infineon/infineon-design-system-vue",
    "link-clean-example-win": "rmdir /s /q node_modules",
    "link-clean-example-unix": "rm -rf node_modules",
    "link-clean-wrapper-package-win": "cd ../../../packages/components-vue && rmdir /s /q node_modules",
    "link-clean-wrapper-package-unix": "cd ../../../packages/components-vue && rm -rf node_modules",
    "link-win": "npm run link-clean-win && npm run link-package",
    "link-unix": "npm run link-clean-example-unix && npm run link-package",
    "link": "node ../../../utils/os-type.js",
    "preview:link": "run-s link dev",
    "test:local": "run-s build:wrapper preview:link"
  },
  "dependencies": {
    "@infineon/infineon-design-system-vue": "^20.36.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "vite": "^4.3.9",
    "vue": "^3.3.10"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  }
}