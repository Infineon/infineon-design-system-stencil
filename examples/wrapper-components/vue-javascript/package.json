{
  "name": "vue-wrapper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:wrapper": "cd ../../../ && npm i && npm run build:components && cd packages/components-vue && npm link ../components && npm run build",
    "link": "npm i && npm link ../../../packages/components-vue",
    "preview:link": "run-s link dev",
    "test:local": "run-s build:wrapper preview:link"
  },
  "dependencies": {
    "@infineon/design-system-tokens": "*",
    "@infineon/infineon-design-system-vue": "^20.36.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "vite": "^4.3.9",
    "vue": "^3.3.10"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  }
}