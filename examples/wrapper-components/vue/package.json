{
  "name": "vue-wrapper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean:dev": "run-s unlink dev",
    "build:wrapper": "npm i && cd ../../../packages/components-vue && npm run build",
    "link": "cd ../../../packages/components-vue && npm link && cd ../../examples/wrapper-components/vue && npm link @infineon/infineon-design-system-vue",
    "unlink": "npm unlink @infineon/infineon-design-system-vue && npm i @infineon/infineon-design-system-vue",
    "preview:link": "run-s link dev",
    "test:local": "run-p build:wrapper preview:link"
  },
  "dependencies": {
    "@infineon/infineon-design-system-vue": "^1.0.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "vite": "^4.1.4"
  }
}