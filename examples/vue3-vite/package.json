{
  "name": "vue3-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:dev": "run-s unlink dev",
    "build": "vite build",
    "preview": "vite preview",
    "build:stencil": "npm i && cd ../ && npm run dev:stencil",
    "link": "cd ../ && npm link && cd ../examples/vue3-vite && npm link @infineon/infineon-design-system-stencil",
    "unlink": "npm unlink @infineon/infineon-design-system-stencil && npm i @infineon/infineon-design-system-stencil",
    "preview:link": "run-s link dev",
    "test:local": "run-p build:stencil preview:link"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "@infineon/infineon-design-system-stencil": "^0.0.63"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0",
    "npm-run-all": "^4.1.5"
  }
}